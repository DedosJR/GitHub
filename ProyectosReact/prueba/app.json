{
  "expo": {
    "name": "MaxiRadioFM",
    "slug": "maxiradiofm",
    "platforms": ["ios", "android", "web"],
    "description": "La mejor radio música esta en MaxiRadioFM desde Tijuana Baja California México",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-512x512-logo.png",
    "userInterfaceStyle": "ligth",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon-96x96.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dedos_dev.maxiradiofm",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-512x512-logo.png",
        "bundleIdentifier": "com.dedos_dev.maxiradiofm",
        "backgroundColor": "#ffffff",
        "permissions": ["FOREGORUND_SERVICE"],
        "config": {
          "googleServices": {
            "backgroundAudio": {
              "enabled": true
            }
          }
        }
      },
      "package": "com.dedos_dev.prueba"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ee539149-8d7f-4001-8c02-e1fbaed20633"
      }
    },
    "owner": "dedos_dev"
  }
}
